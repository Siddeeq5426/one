<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>My E-Commerce Store</title>
  <link rel="stylesheet" href="style.css"> <!-- External CSS -->
</head>
<body>
  <header>
    <h1>My E-Commerce Store</h1>
    <p>Best Deals on Mobiles, Laptops & Clothes</p>
  </header>

  <nav>
    <a href="#mobiles">Mobiles</a>
    <a href="#laptops">Laptops</a>
    <a href="#clothes">Clothes</a>
  </nav>

  <!-- Mobiles -->
  <section id="mobiles">
    <h2>Mobiles</h2>
    <div class="products" id="mobiles-list"></div>
  </section>

  <!-- Laptops -->
  <section id="laptops">
    <h2>Laptops</h2>
    <div class="products" id="laptops-list"></div>
  </section>

  <!-- Clothes -->
  <section id="clothes">
    <h2>Clothes</h2>
    <div class="products" id="clothes-list"></div>
  </section>

  <script src="script.js"></script> <!-- External JavaScript -->
</body>
</html>
